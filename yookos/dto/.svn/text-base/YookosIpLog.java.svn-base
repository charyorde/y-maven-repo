package com.yookos.dto;

import java.util.Date;

/**
 * @author Msawenkosi Ntuli
 *
 */
public class YookosIpLog {

	private long yookosIpLogId;	
	private long userID;
	private String ipAddress;
	private String userAgent;
	private String isMobileDevice;
	private long dateTime;
	private long logType;

	public YookosIpLog(long userID, String ipAddress,
			String userAgent, String isMobileDevice, long dateTime, long logType) {

		this.userID = userID;
		this.ipAddress = ipAddress;
		this.userAgent = userAgent;
		this.isMobileDevice = isMobileDevice;
		this.dateTime = dateTime;
		this.logType = logType;
	}
	
	public YookosIpLog(long yookosIpLogId, long userID, String ipAddress,
			String userAgent, String isMobileDevice, long dateTime, long logType) {

		this.yookosIpLogId = yookosIpLogId;
		this.userID = userID;
		this.ipAddress = ipAddress;
		this.userAgent = userAgent;
		this.isMobileDevice = isMobileDevice;
		this.dateTime = dateTime;
		this.logType = logType;
	}
	
	public long getYookosIpLogId() {
		return yookosIpLogId;
	}
	public void setYookosIpLogId(long yookosIpLogId) {
		this.yookosIpLogId = yookosIpLogId;
	}
	public long getUserID() {
		return userID;
	}
	public void setUserID(long userID) {
		this.userID = userID;
	}
	public String getIpAddress() {
		return ipAddress;
	}
	public void setIpAddress(String ipAddress) {
		this.ipAddress = ipAddress;
	}
	public String getUserAgent() {
		return userAgent;
	}
	public void setUserAgent(String userAgent) {
		this.userAgent = userAgent;
	}
	public String getIsMobileDevice() {
		return isMobileDevice;
	}
	public void setIsMobileDevice(String isMobileDevice) {
		this.isMobileDevice = isMobileDevice;
	}

	public long getDateTime() {
		return dateTime;
	}

	public void setDateTime(long dateTime) {
		this.dateTime = dateTime;
	}

	public long getLogType() {
		return logType;
	}

	public void setLogType(long logType) {
		this.logType = logType;
	}
}
