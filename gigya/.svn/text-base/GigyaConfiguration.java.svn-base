package com.jivesoftware.community.aaa.sso.gigya;

import com.jivesoftware.community.aaa.sso.SSOSchemeConfiguration;

public interface GigyaConfiguration extends SSOSchemeConfiguration {

    /**
     * The application ID that is given by Gigya
     *
     * @return application id
     */
    String getApplicationID();

    void setApplicationID(String applicationID);

    /**
     * The application secret provided by Gigya
     *
     * @return application secret
     */
    String getApplicationSecret();

    void setApplicationSecret(String secret);
    
    
    /**
     * The api key provided by Gigya
     * 
     * @return api key
     */
    String getApiKey();
    
    void setApiKey(String apiKey);

}
